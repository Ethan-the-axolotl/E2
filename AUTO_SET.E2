@name AUTO_SETv2
@inputs 
@outputs 
@persist 
@trigger 
interval(5)
A="ulx hp \""+owner():name()+"\" 500"
B="ulx armor \""+owner():name()+"\" 255"
if(changed(owner():health())&owner():health()<225&owner():health()>0){concmd(A)}
if(changed(owner():armor())&owner():armor()<100&owner():armor()>=0){concmd(B)}
